{
  "name": "velkuns/adventofcode-2024",
  "description": "Year 2023 - CLI Application for adventofcode.com challenges",
  "type": "project",
  "minimum-stability": "stable",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Application\\": "./src",
      "Application\\Script\\": "./scripts"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Application\\Tests\\Unit\\": "./tests/unit",
      "Application\\Tests\\Integration\\": "./tests/integration"
    }
  },

  "require": {
    "php": "8.3.*||8.4.*",
    "ext-ctype": "*",
    "ext-json":"*",

    "eureka/kernel-console": "^6.0",
    "eureka/component-console": "^6.1",
    "velkuns/math": "^1.3",
    "lcobucci/clock": "^3.3.1",
    "psr/container": "^2.0.2"
  },

  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65.0",
    "phpstan/phpstan": "^1.12.12",
    "phpstan/phpstan-phpunit": "^1.4.1",
    "phpunit/phpcov": "^9.0.2",
    "phpunit/phpunit": "^10.5.38",
    "shipmonk/composer-dependency-analyser": "^1.8.1"
  }
}
